<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterPersonal</servlet-name>
        <servlet-class>controller.RegisterPersonal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterNave</servlet-name>
        <servlet-class>controller.RegisterNave</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterKontrol</servlet-name>
        <servlet-class>controller.RegisterKontrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterMision</servlet-name>
        <servlet-class>controller.RegisterMision</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetNaves</servlet-name>
        <servlet-class>controller.GetNaves</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controller.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletePersonal</servlet-name>
        <servlet-class>controller.DeletePersonal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletePersonalForm</servlet-name>
        <servlet-class>controller.DeletePersonalForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckExit</servlet-name>
        <servlet-class>controller.CheckExit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewExecution</servlet-name>
        <servlet-class>controller.NewExecution</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterPersonal</servlet-name>
        <url-pattern>/RegisterPersonal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterNave</servlet-name>
        <url-pattern>/RegisterNave</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterKontrol</servlet-name>
        <url-pattern>/RegisterKontrol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterMision</servlet-name>
        <url-pattern>/RegisterMision</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetNaves</servlet-name>
        <url-pattern>/GetNaves</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletePersonal</servlet-name>
        <url-pattern>/DeletePersonal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletePersonalForm</servlet-name>
        <url-pattern>/DeletePersonalForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckExit</servlet-name>
        <url-pattern>/CheckExit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewExecution</servlet-name>
        <url-pattern>/NewExecution</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
